{
    "name": "storybook-contrast",
    "version": "0.11.1",
    "main": "dist/index.js",
    "description": "Contrast storybook integration",
    "repository": "https://github.com/toyboxsystems/storybook-contrast.git",
    "license": "MIT",
    "author": "Contrast <support@contrast.app>",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "npm run clean && NODE_ENV=development babel src --ignore src/__tests__,src/__mocks__ --out-dir dist -s --extensions \".js,.jsx,.ts,.tsx\" ",
        "demo": "npm run clean && NODE_ENV=development IS_DEMO=true babel src --ignore src/__tests__,src/__mocks__ --out-dir dist -s --extensions \".js,.jsx,.ts,.tsx\" ",
        "prod": "npm run clean && NODE_ENV=production babel src --ignore src/__tests__,src/__mocks__ --out-dir dist -s --extensions \".js,.jsx,.ts,.tsx\" ",
        "lint": "eslint src --ext .js --cache",
        "test": "jest",
        "watch": "watch 'yarn run dev' ./src",
        "example": "start-storybook -p 3003 -c ./example/.storybook -s ./example",
        "demo": "IS_DEMO=true start-storybook -p 3003 -c ./example/.storybook -s ./example",
        "deploy": "yarn run prod && yarn version --patch && cp README.md dist/README.md && cp package.json dist/package.json && yarn publish dist"
    },
    "dependencies": {
        "@babel/preset-typescript": "^7.10.4",
        "@babel/standalone": "^7.0.0",
        "@storybook/addon-essentials": "^6.1.5",
        "@storybook/addons": "^6.0.21",
        "@storybook/api": "^6.0.21",
        "@storybook/components": "^6.0.21",
        "@storybook/react": "6.0.20",
        "@storybook/source-loader": "^6.0.26",
        "codemirror": "^5.40.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.20"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/node": "^7.10.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/preset-typescript": "^7.10.1",
        "@storybook/addon-storysource": "^6.0.26",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.5.0",
        "eslint-plugin-flowtype": "^2.46.1",
        "eslint-plugin-jest": "^21.22.0",
        "eslint-plugin-no-only-tests": "^2.0.0",
        "eslint-plugin-react": "^7.7.0",
        "flow-bin": "^0.80.0",
        "jest": "^23.5.0",
        "mensch": "^0.3.3",
        "react": "^16.3.0",
        "react-dom": "^16.5.0",
        "rimraf": "^2.6.2",
        "watch": "^1.0.2"
    },
    "peerDependencies": {
        "react": "^15 || ^16"
    },
    "keywords": ["storybook", "addon", "react", "flow", "editor", "live"],
    "private": false
}
